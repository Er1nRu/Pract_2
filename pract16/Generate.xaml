<Page x:Class="pract16.Generate"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pract16"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Generate">

    <Grid>
        <StackPanel>
            <Label x:Name="count" Content="кОЛ"/>
            <Slider x:Name="sld" Value="1" Maximum="100" TickFrequency="5" Margin="10 0" ValueChanged="sld_ValueChanged" SmallChange="1"/>
            <Button Content="Сгенерировать пользователей" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Page>
